		startup-position = /bits/ 8 <PURE_DDR>; /*host dsp start mode : DDR*/
		memory-region = <&dsp_fw_reserved1>;
		reg-names = "base-reg",
			    "ddrfw-region";
		reg = <0xff690000 0x114>, /*host base reg address*/
