			 <&mbox_pl1 TM2_DSPA2REE_DEV>,
			 <&mbox_pl1 TM2_REE2DSPB_DEV>,
			 <&mbox_pl1 TM2_DSPB2REE_DEV>;
		mbox-dests = <MAILBOX_AOCPU>,
			     <MAILBOX_DSP>,
